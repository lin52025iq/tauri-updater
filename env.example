# 服务器配置
NODE_ENV=production
PORT=3000

# GitHub 配置
# GitHub Personal Access Token (需要 repo 权限)
GITHUB_TOKEN=your_github_token_here
# GitHub 仓库所有者
GITHUB_OWNER=your_github_username
# GitHub 仓库名称
GITHUB_REPO=your_repository_name

# 阿里云 OSS 配置
# 阿里云 Access Key ID
ALIOSS_ACCESS_KEY_ID=your_access_key_id
# 阿里云 Access Key Secret
ALIOSS_ACCESS_KEY_SECRET=your_access_key_secret
# OSS 区域
ALIOSS_REGION=oss-cn-hangzhou
# OSS 存储桶名称
ALIOSS_BUCKET=your_bucket_name
